
<h1>Restaurants</h1>

<ul>
  <% @restaurants.each do |restaurant| %>
    <li> <%= link_to restaurant.name , restaurant_path(restaurant) %> - <%= restaurant.category %>
    </li>
      <p> <%= restaurant.address %> (<%= (restaurant.phone_number) %>)</p>
  <% end %>
</ul>


<%= link_to "Add a new restaurant", new_restaurant_path, class:"btn btn-primary" %>


